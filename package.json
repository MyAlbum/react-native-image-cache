{
  "name": "@georstat/react-native-image-cache",
  "version": "1.1.1",
  "access": "public",
  "description": "React Native image file system caching for iOS and Android",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "files": [
    "src",
    "lib"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android",
    "image-cache",
    "cache",
    "file system image cache"
  ],
  "repository": "https://github.com/georstat/react-native-image-cache",
  "author": "Efstathios Ntonas - George Bakogiannis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georstat/react-native-image-cacheissues"
  },
  "homepage": "https://github.com/georstat/react-native-image-cache",
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && yarn copy-dts",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib/typescript",
    "release": "release-it",
    "example": "yarn --cwd example",
    "prettier": "prettier \"**/*.{ts,tsx,js}\" --write",
    "bootstrap": "yarn install && yarn example"
  },
  "dependencies": {
    "react-native-file-access": "^1.6.1",
    "lodash": "^4.17.21",
    "crypto-js": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@react-native-community/bob": "^0.17.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.1",
    "@types/react": "^17.0.5",
    "@types/lodash": "^4.14.168",
    "@types/react-native": "0.64.5",
    "auto-changelog": "^2.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "react": "~17.0.2",
    "react-native": "^0.64.1",
    "release-it": "^14.6.2",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-file-access": "^1.6.1",
    "lodash": "^4.17.21",
    "crypto-js": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
